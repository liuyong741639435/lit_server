
.fellow{
	width: 100%;
}
.fellow>view:not(:last-child){
	border-bottom:1px solid #8F8F94 ;
}
.fellow>view{
	width:100%;
	height:100rpx;
	background: #eef;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
}
.fellow>view>image{
	width: 100rpx;height: 100rpx;
	margin: 0 20rpx;
	border-radius: 20%;
	overflow: hidden;
}
.fellow>view>text{
	font-size: 70rpx;
	line-height: 100rpx;
}


